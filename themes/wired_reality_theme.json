        "foreground": "#60706F"
      }
    },
    {
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#DB5252"
      }
    },
    {
      "scope": [
        "keyword.control.class",
        "keyword.control.def",
        "keyword.control.function",
        "keyword",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#DB5252"
      }
    },
    {
      "scope": [
        "meta.function.call",
        "entity.name.function",
        "support.function",
